{
    "globals": {
        "angular": true
    },
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-comma-dangle": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "block-scoped-var": 0,
        "complexity": [ 2, 10 ],
        "consistent-return": 2,
        "curly": [ 0, "all" ],
        "default-case": 2,
        "dot-notation": 0,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-labels": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 0,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 2,
        "no-void": 2,
        "no-warning-comments": [ 2, { "terms": [ "fixme", "wtf" ], "location": "anywhere" } ],
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "wrap-iife": [ 2, "outside" ],
        "yoda": [ 2, "never" ],
        "strict": 2,
        "no-extra-strict": 2,
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-undef-init": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 0,
        "handle-callback-err": [ 2, "^(e$|(e|(.*(_e|E)))rr)" ],
        "no-mixed-requires": 0,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-sync": 0,
        "camelcase": 2,
        "consistent-this": [ 0, "self" ],
        "eol-last": 2,
        "func-names": 0,
        "func-style": 0,
        "new-cap": 0,
        "new-parens": 2,
        "no-nested-ternary": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-space-before-semi": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 2,
        "no-wrap-func": 2,
        "no-mixed-spaces-and-tabs": 2,
        "quotes": [ 2, "single" ],
        "quote-props": 0,
        "semi": [ 2, "always" ],
        "sort-vars": 0,
        "max-nested-callbacks": [ 2, 5 ],
        "one-var": 0,
        "wrap-regex": 0,
        "max-depth": [ 2, 4 ],
        "max-len": [ 2, 120, 4 ],
        "max-params": [ 0, 6 ],
        "max-statements": [ 2, 30 ],
        "no-bitwise": 0,
        "no-plusplus": 2
    }
}
